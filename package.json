{
  "name": "@snappjack/sdk-js",
  "version": "0.0.1",
  "description": "TypeScript SDK for Snappjack - Enable your app for AI agents",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && npm run build:browser",
    "build:browser": "rollup -c rollup.config.mjs",
    "build:watch": "tsc --watch",
    "dev": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "snappjack",
    "ai",
    "agents",
    "mcp",
    "sdk",
    "typescript",
    "browser"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "@rollup/plugin-typescript": "^12.1.1",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "rollup": "^4.25.0",
    "ts-jest": "^29.4.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snappjack/snappjack-sdk-js.git"
  }
}