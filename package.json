{
  "name": "@snappjack/sdk-js",
  "version": "0.5.1",
  "description": "TypeScript SDK for Snappjack - Enable your app for AI agents with secure JWT authentication",
  "main": "dist/client.js",
  "module": "dist/client.mjs",
  "types": "dist/client.d.ts",
  "exports": {
    ".": {
      "types": "./dist/client.d.ts",
      "browser": {
        "import": "./dist/client.mjs",
        "require": "./dist/snappjack-sdk.js"
      },
      "node": {
        "import": "./dist/client.mjs",
        "require": "./dist/client.js"
      },
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    },
    "./client": {
      "types": "./dist/client.d.ts",
      "browser": {
        "import": "./dist/client.mjs",
        "require": "./dist/snappjack-sdk.js"
      },
      "node": {
        "import": "./dist/client.mjs",
        "require": "./dist/client.js"
      },
      "import": "./dist/client.mjs",
      "require": "./dist/client.js"
    },
    "./server": {
      "types": "./dist/server.d.ts",
      "node": {
        "import": "./dist/server.mjs",
        "require": "./dist/server.js"
      },
      "import": "./dist/server.mjs",
      "require": "./dist/server.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "NODE_ENV=production tsc && npm run build:browser",
    "build:browser": "NODE_ENV=production rollup -c rollup.config.mjs",
    "build:dev": "NODE_ENV=development tsc && npm run build:browser:dev",
    "build:browser:dev": "NODE_ENV=development rollup -c rollup.config.mjs",
    "dev": "concurrently \"tsc --watch\" \"NODE_ENV=development rollup -c rollup.config.mjs --watch\"",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "test:coverage": "jest --ci --coverage",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "snappjack",
    "ai",
    "agents",
    "mcp",
    "sdk",
    "typescript",
    "browser"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-replace": "^6.0.2",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.1.0",
    "concurrently": "^9.2.0",
    "jest": "^30.0.5",
    "jest-environment-jsdom": "^30.0.5",
    "rollup": "^4.25.0",
    "ts-jest": "^29.4.0",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/snappjack/snappjack-sdk-js.git"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "dotenv": "^17.2.2"
  }
}
